### RHDH VARIABLES ###
######################

# The namespace where RHDH is installed.
export DEPLOYMENT_NAMESPACE=

# This is the name of your Backstage CR that is located in the $DEPLOYMENT_NAMESPACE.
export BACKSTAGE_CR_NAME=

### LIGHTSPEED CORE VARIABLES ###
#################################

# This is the image registry where your built Lightspeed Core image is located.
# If unset, defaults to: quay.io/lightspeed-core/lightspeed-stack:dev-latest
export LCS_IMAGE=

# This is the image registry where your built Llama Stack image is located.
# If unset, defaults to: quay.io/redhat-ai-dev/llama-stack:latest
export LLS_IMAGE=

# This is the image registry where your built RHDH RAG data is located.
# If unset, defaults to: quay.io/redhat-ai-dev/rag-content:release-1.7-lcs
export RAG_IMAGE=

### FEEDBACK HARVESTER VARIABLES ###
####################################

# This is the image registry where your built Feedback Harvester image is located.
# If unset, defaults to: quay.io/redhat-ai-dev/feedback-harvester:latest
export HARVESTER_IMAGE=
# If unset, defaults to 15 minutes.
export FETCH_FREQUENCY=

### POSTGRES VARIABLES ###
##########################

# All vars below required for
# Postgres deployments

export PGUSER=
export PGPASSWORD=
export PGDATABASE=