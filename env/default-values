# [REQUIRED] The namespace where RHDH is installed.
export DEPLOYMENT_NAMESPACE=

# [REQUIRED] This is the image registry where your built Lightspeed Core image is located.
export LCS_IMAGE=

# [REQUIRED] This is the name of your Backstage CR that is located in the $DEPLOYMENT_NAMESPACE.
export BACKSTAGE_CR_NAME=

# [REQUIRED] The URL of your vLLM server.
export VLLM_URL=

# [REQUIRED] The token for accessing your vLLM server.
export VLLM_API_KEY=
